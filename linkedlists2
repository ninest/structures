class LinkedList:
    def __init__(self, *args):
        self.list = []

    def add(self, data):
        self.list.append(data)

    def insert(self, data, position):
        self.list.insert(position, data)

    def get_next(self, data):
        i = self.list.index(data)
        return self.list[i + 1]

    def get_previous(self, data):
        i = self.list.index(data)
        return self.list[i - 1]

    def del_data(self, data):
        self.list.pop(self.list.index(data))

    def add_list(self, *args):
        for a in args:
            self.list.append(a)

    def search(self, data):
        if data in self.list:
            return self.list.index(data)
        else:
            return False

    def full_list(self):
        toreturn = ''
        for i in range(0,len(self.list)-1):
            toreturn += "{},{}-> ".format(self.list[i], self.list[i + 1])
        return toreturn
        # for i in self.list:
        #     print(i)

            

# EXAMPLE:
main_list = LinkedList()
main_list.add_list('a', 'b', 'c', 'd', 'e', 'f')

print(main_list.search('a'))

main_list.del_data('a')
print(main_list.search('a')) # False


print(main_list.get_next('d')) 

print(main_list.get_next('f')) # returns error

